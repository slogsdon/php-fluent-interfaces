{
    "name": "slogsdon/php-fluent-interfaces",
    "description": "some ways to create Fluent Interfaces in PHP",
    "version": "0.0.1",
    "type": "library",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4",
        "squizlabs/php_codesniffer": "~2.1",
        "satooshi/php-coveralls": "dev-master",
        "sebastianbergmann/phpcov": "1.1.0",
        "phpdocumentor/phpdocumentor": "2.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Shane Logsdon",
            "email": "shane@shanelogsdon.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "ABC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ABC\\Test\\": "tests/"
        }
    }
}
